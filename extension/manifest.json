{
    "name": "LobbyAlarm",
    "version": "0.1.0",
    "description":
        "Lobbyismus aufdecken. Verlinken zu LobbyPedia (LobbyControl).",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["data.js", "popper.min.js", "tooltip.min.js", "detect.js"],
            "css": ["style.css"],
            "exclude_globs": ["https://lobbypedia.de/*"],
            "run_at": "document_end"
        }
    ]
}
